    "pino": "^4.16.1",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-router-dom": "^4.2.2",
    "styled-components": "^3.2.6",
    "uws": "^9.147.0"
  },
  "devDependencies": {
    "@types/node": "^9.6.6",
    "prettier": "^1.12.1",
    "ts-node": "^6.0.0",
